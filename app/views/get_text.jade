.row
  include shared/dashboard_nav.jade
  .col.s12.l10.center
    h5 Paste your text
    .row
      .input-field.col.s12.offset-l2.l8
        textarea#paste-textarea.materialize-textarea
        label(for='paste-textarea') Textarea
      .col.s12.input-field.offset-l2.l8
        select(ng-model="chunkSize")
          option(value='1') one word at a time
          option(value='2') Two words at a time
          option(value='3') Three word at a time
        label Chunk Size
      .col.s12.input-field.offset-l2.l8
        select(ng-model="speed")
          option(value='300') 300
          option(value='320') 320
          option(value='340') 340
          option(value='360') 360
        label Words per Minute
      .col.s12.input-field.offset-l2.l8
        button.btn.waves-effect.waves-light(ng-click="speedRead()")
          i.material-icons.right send
          | Speed Read
      script.
        $(document).ready(function() {
          $('select').material_select();
        })




